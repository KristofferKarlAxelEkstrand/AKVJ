{
	"name": "akvj",
	"version": "0.0.0",
	"description": "A Vite project for AKVJ",
	"main": "main.js",
	"type": "module",
	"private": false,
	"scripts": {
		"dev": "vite",
		"generate-animation-json-to-json": "node generateAnimationsJson.js",
		"watch:animations": "chokidar \"src/public/animations/**/*\" -c \"npm run generate-animation-json-to-json\"",
		"build": "vite build --emptyOutDir",
		"preview": "vite preview",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"format:stylelint": "stylelint \"**/*.css\" --fix",
		"format:prettier": "prettier --write .",
		"fix": "npx npm-check-updates -u && npm install && npm audit fix --force && npm prune && npm install && npm update"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/KristofferKarlAxelEkstrand/AKVJ"
	},
	"author": {
		"name": "Kristoffer Ekstrand",
		"email": "hej@kristofferekstrand.se",
		"url": "https://kristofferekstrand.se"
	},
	"license": "MIT",
	"devDependencies": {
		"@eslint/js": "^9.39.1",
		"chokidar-cli": "^3.0.0",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-prettier": "^5.5.4",
		"globals": "^16.5.0",
		"npm-run-all": "^4.1.5",
		"prettier": "3.7.4",
		"stylelint": "^16.26.1",
		"stylelint-config-recommended": "^17.0.0",
		"stylelint-order": "^7.0.0",
		"vite": "^7.2.6"
	}
}
